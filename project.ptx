<?xml version="1.0" encoding="utf-8"?>
<!--
    This file, the project manifest, provides the overall configuration for
    your PreTeXt project. To edit the content of your document, open
    `source/main.ptx`. See
    https://pretextbook.org/doc/guide/html/processing-CLI.html#cli-project-manifest.
-->
<project ptx-version="2">
  <targets>
    <target name="web-a" format="html" source="main-a.ptx" deploy-dir="web-a"/>
    <target name="web-b" format="html" source="main-b.ptx" deploy-dir="web-b"/>
    <target name="web-c" format="html" source="main-c.ptx" deploy-dir="web-c"/>
    <target name="print-a" format="pdf" source="main-a.ptx" deploy-dir="web-a/pdf" output-filename="print.pdf"/>
    <target name="print-b" format="pdf" source="main-b.ptx" deploy-dir="web-b/pdf" output-filename="print.pdf"/>
    <target name="print-c" format="pdf" source="main-c.ptx" deploy-dir="web-c/pdf" output-filename="print.pdf"/>
  </targets>
</project>
